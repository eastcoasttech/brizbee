<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Brizbee.Mobile.Views.InConfirmPage"
             xmlns:vm="clr-namespace:Brizbee.Mobile.ViewModels"
             Title="{Binding Title}">
    <ContentPage.BindingContext>
        <vm:InConfirmViewModel />
    </ContentPage.BindingContext>
    <ContentPage.Content>
        <StackLayout Padding="40" BackgroundColor="#f5f5f5">
            <Label Text="Please Confirm" />
            <Button Text="Save and Continue" x:Name="btnContinue" />
            <Button Text="Cancel" x:Name="btnCancel" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>