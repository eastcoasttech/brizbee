<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Brizbee.Mobile.Views.InTaskPage"
             xmlns:vm="clr-namespace:Brizbee.Mobile.ViewModels">
    <ContentPage.BindingContext>
        <vm:InTaskViewModel />
    </ContentPage.BindingContext>
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Welcome to Xamarin.Forms!" />
            <Button Text="Continue" x:Name="btnContinue" />
            <Button Text="Cancel" x:Name="btnCancel" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>